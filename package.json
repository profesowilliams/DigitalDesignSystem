{
  "name": "react-vite-storybook",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "build": "rimraf dist && NODE_ENV=production BABEL_ENV=production rollup -c",
    "lint": "yarn check-types && yarn eslint && yarn stylelint",
    "lint:fix": "yarn eslint:fix && yarn stylelint:fix",
    "eslint": "eslint . --ext .js,.jsx,.tsx,.ts",
    "eslint:fix": "eslint . --ext .js,.jsx,.tsx,.ts --fix",
    "stylelint": "stylelint \"**/*.{css,scss,sass}\"",
    "stylelint:fix": "stylelint \"**/*.{css,scss,sass}\" --fix",
    "check-types": "tsc --noEmit true",
    "ci": "yarn lint && yarn test --watch=false",
    "start": "NODE_ENV=development BABEL_ENV=development yarn storybook",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "storybook:deploy": "yarn storybook:build && touch ./storybook-static/.nojekyll && gh-pages -d ./storybook-static -t true",
    "chromatic": "npx chromatic",
    "test": "jest",
    "test:storybook": "start-storybook -p 6006 & jest",
    "rollup": "npx rollup --config rollup.config.mjs"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "keywords": [
    "react",
    "bootstrap",
    "storybook",
    "react-bootstrap",
    "web components"
  ],
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:storybook/recommended"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "peerDependencies": {
    "clsx": "2.x",
    "react": "18.x",
    "react-dom": "18.x"
  },
  "dependencies": {
    "-": "^0.0.1",
    "bootstrap": "^5.2.0",
    "bootstrap-icons": "^1.9.1",
    "formik": "^2.2.9",
    "is-ci": "^3.0.1",
    "lodash": "^4.17.21",
    "react": "18.x",
    "react-bootstrap": "^2.9.0",
    "react-dom": "18.x",
    "react-scripts": "4.0.3",
    "react-slider": "^2.0.6",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.8.3",
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.17.8",
    "@babel/preset-env": "^7.14.9",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.23.3",
    "@jest/globals": "^29.7.0",
    "@playwright/test": "^1.41.1",
    "@r2wc/react-to-web-component": "2.0.2",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^8.5.0",
    "@rollup/plugin-url": "^8.0.2",
    "@storybook/addon-a11y": "^7.6.2",
    "@storybook/addon-actions": "^7.6.3",
    "@storybook/addon-coverage": "^1.0.0",
    "@storybook/addon-docs": "^7.6.3",
    "@storybook/addon-essentials": "^7.6.3",
    "@storybook/addon-interactions": "^7.6.3",
    "@storybook/addon-links": "^7.6.3",
    "@storybook/addon-onboarding": "^1.0.9",
    "@storybook/addon-storyshots": "^7.6.10",
    "@storybook/addons": "^7.6.7",
    "@storybook/blocks": "^7.6.3",
    "@storybook/builder-vite": "^7.6.7",
    "@storybook/node-logger": "^7.6.3",
    "@storybook/preset-create-react-app": "^3.2.0",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^7.6.7",
    "@storybook/react-vite": "^7.6.7",
    "@storybook/test": "^7.6.3",
    "@storybook/test-runner": "^0.16.0",
    "@svgr/rollup": "^8.1.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "*",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.5",
    "@types/react": "^17.0.75",
    "@types/react-dom": "^17.0.10",
    "@vitejs/plugin-react": "^1.0.7",
    "@web/rollup-plugin-copy": "^0.3.0",
    "@web/rollup-plugin-html": "^1.11.0",
    "@web/rollup-plugin-polyfills-loader": "^1.3.1",
    "@web/test-runner": "^0.15.0",
    "airbnb-browser-shims": "^3.3.0",
    "axe-playwright": "^1.1.12",
    "babel-loader": "^8.2.4",
    "chromatic": "^10.0.0",
    "classnames": "^2.5.1",
    "clsx": "^2.1.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.8.1",
    "eslint": "^7.0.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-lit": "^1.8.3",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.15",
    "eslint-plugin-wc": "^1.5.0",
    "gh-pages": "^3.2.3",
    "husky": "^8.0.3",
    "jest": "^26.6.3",
    "lint-staged": "^13.2.2",
    "postcss": "^8.4.31",
    "postcss-scss": "^4.0.9",
    "prettier": "^2.8.8",
    "prop-types": "^15.8.1",
    "react-bootstrap-typeahead": "^6.3.2",
    "resolve-url-loader": "^5.0.0",
    "rollup": "^3.10.1",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-dts": "^6.1.0",
    "rollup-plugin-minify-html-literals": "^1.2.6",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-summary": "^2.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.69.7",
    "storybook": "^7.6.10",
    "stylelint-config-standard": "^33.0.0",
    "stylelint-config-standard-scss": "^9.0.0",
    "ts-jest": "^29.1.2",
    "typescript": "^4.9.5",
    "vite": "^3.0.0",
    "vite-plugin-svgr": "^3.2.0",
    "vitest": "^0.34.6",
    "yarn": "^1.22.21"
  }
}
