    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-summary": "^2.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.69.7",
    "storybook": "^7.6.10",
    "stylelint-config-standard": "^33.0.0",
