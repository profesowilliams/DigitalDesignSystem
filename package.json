{
  "name": "htmlvite",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "storybook": "concurrently \"npm run storybook:lit\" \"npm run storybook:react\" \"npx wait-on tcp:6007 && npm run storybook:html\"",
    "storybook:html": "CACHE_DIR=\"../.cache/storybook/html\" storybook dev -p 6006",
    "storybook:react": "CACHE_DIR=\"../.cache/storybook/react\" storybook dev -p 6007 -c .storybook/react --no-open",
    "storybook:lit": "CACHE_DIR=\"../.cache/storybook/lit\" storybook dev -p 6008 -c .storybook/lit --no-open",
    "build-storybook": "storybook build",
    "build:react": "vite build --config vite.config.react.js",
    "build:lit": "vite build --config vite.config.lit.js",
    "build": "npm run build:react && npm run build:lit"
  },
