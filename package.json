{
  "name": "htmlvite",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "scripts": {
    "chromatic": "npx chromatic --build-script-name storybook:build",
    "chromatic:lit": "npx chromatic --build-script-name storybook:build:lit --config chromatic.config.lit.json --project-token $CHROMATIC_PROJECT_TOKEN_1 ",
    "chromatic:react": "source .env && npx chromatic --build-script-name storybook:build:react --config chromatic.config.react.json --project-token $CHROMATIC_PROJECT_TOKEN_2",
    "lint": "yarn check-types && yarn eslint && yarn stylelint",
    "lint:fix": "yarn eslint:fix && yarn stylelint:fix",
    "eslint": "eslint . --ext .js,.jsx,.tsx,.ts",
    "eslint:fix": "eslint . --ext .js,.jsx,.tsx,.ts --fix",
    "stylelint": "stylelint \"**/*.{css,scss,sass}\"",
    "stylelint:fix": "stylelint \"**/*.{css,scss,sass}\" --fix",
    "check-types": "tsc --noEmit true",
    "ci": "yarn lint && yarn test --watch=false",
    "start": "NODE_ENV=development BABEL_ENV=development yarn storybook",
    "test": "wtr --config web-test-runner.config.mjs",
    "test:watch": "wtr --config web-test-runner.config.mjs --node-resolve --watch",
    "storybook": "concurrently \"npm run storybook:lit\" \"npm run storybook:react\" \"npx wait-on tcp:6007 && npm run storybook:html\"",
    "storybook:build": "storybook build",
    "storybook:build:lit": "npx storybook build -c .storybook/lit",
    "storybook:build:react": "npx storybook build -c .storybook/react",
    "storybook:html": "CACHE_DIR=\"../.cache/storybook/html\" storybook dev -p 6006 -c .storybook/html",
    "storybook:react": "CACHE_DIR=\"../.cache/storybook/react\" storybook dev -p 6007 -c .storybook/react --no-open",
    "storybook:lit": "CACHE_DIR=\"../.cache/storybook/lit\" storybook dev -p 6008 -c .storybook/lit --no-open",
    "build:lit:development": "vite build --config vite.config.lit.mts --mode development && npm run build:css:development",
    "build:lit:production": "vite build --config vite.config.lit.mts --mode production && npm run build:css:production",
    "build:css:development": "node build-css.cjs --env development",
    "build:css:production": "node build-css.cjs --env production"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "author": "",
  "license": "ISC",
  "keywords": [
    "react",
    "bootstrap",
    "storybook",
    "lit",
    "web components"
  ],
  "engineStrict": true,
  "engines": {
    "npm": ">=10.0.0",
    "node": ">=22.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
