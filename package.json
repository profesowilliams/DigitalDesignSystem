{
  "name": "react-vite-storybook",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "start": "storybook dev -p 6006 -s public",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "deploy": "storybook build && gh-pages -d storybook-static",
    "chromatic": "npx chromatic",
    "build": "npm run clean && cross-env NODE_ENV=production babel src/components --out-dir dist --copy-files --no-copy-ignored"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "react",
    "bootstrap",
    "storybook",
    "react-bootstrap",
    "web components"
  ],
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:storybook/recommended"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "peerDependencies": {
    "react": "18",
    "react-dom": "18"
  },
  "dependencies": {
    "@r2wc/react-to-web-component": "2.0.2",
    "bootstrap": "^5.2.0",
    "bootstrap-icons": "^1.9.1",
    "formik": "^2.2.9",
    "is-ci": "^3.0.1",
    "lodash": "^4.17.21",
    "react": "18",
    "react-bootstrap": "^2.9.0",
    "react-dom": "18",
    "react-scripts": "4.0.3",
    "react-slider": "^2.0.6",
    "webpack": "^5.89.0",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.17.8",
    "@babel/preset-env": "^7.14.9",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.23.3",
    "@rollup/plugin-typescript": "^11.1.5",
    "@storybook/addon-a11y": "^7.6.2",
    "@storybook/addon-actions": "^7.6.3",
    "@storybook/addon-coverage": "^1.0.0",
    "@storybook/addon-docs": "^7.6.3",
    "@storybook/addon-essentials": "^7.6.3",
    "@storybook/addon-interactions": "^7.6.3",
    "@storybook/addon-links": "^7.6.3",
    "@storybook/addon-onboarding": "^1.0.9",
    "@storybook/addon-storyshots": "^7.6.3",
    "@storybook/addons": "^7.6.7",
    "@storybook/blocks": "^7.6.3",
    "@storybook/builder-vite": "^7.6.7",
    "@storybook/node-logger": "^7.6.3",
    "@storybook/preset-create-react-app": "^3.2.0",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^7.6.7",
    "@storybook/react-vite": "^7.6.7",
    "@storybook/test": "^7.6.3",
    "@storybook/test-runner": "^0.16.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "*",
    "@testing-library/user-event": "^12.1.10",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@vitejs/plugin-react": "^1.0.7",
    "@web/rollup-plugin-copy": "^0.3.0",
    "@web/rollup-plugin-html": "^1.11.0",
    "@web/rollup-plugin-polyfills-loader": "^1.3.1",
    "@web/test-runner": "^0.15.0",
    "airbnb-browser-shims": "^3.3.0",
    "axe-playwright": "^1.1.12",
    "babel-loader": "^8.2.4",
    "chromatic": "^10.0.0",
    "classnames": "^2.5.1",
    "cross-env": "^7.0.3",
    "css-loader": "^6.8.1",
    "eslint": "^7.0.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-lit": "^1.8.3",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.15",
    "eslint-plugin-wc": "^1.5.0",
    "eslint-webpack-plugin": "^2.7.0",
    "gh-pages": "^3.2.3",
    "husky": "^8.0.3",
